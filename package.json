{
  "name": "typescript-babel-eslint",
  "version": "0.0.1",
  "description": "typescript babel eslint template project",
  "main": "src/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext .js,.ts --fix",
    "checktypes": "tsc --noEmit",
    "checktypes:watch": "npm run checktype -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "npx babel src --out-dir dist --source-maps inline --extensions .ts,.tsx",
    "bundle": "webpack",
    "dev": "nodemon --watch 'src/' -e ts --exec 'ts-node' ./src/app.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betgar/typescript-babel-eslint.git"
  },
  "keywords": [
    "typescript",
    "babel",
    "eslint",
    "starter"
  ],
  "author": "betgar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/betgar/typescript-babel-eslint/issues"
  },
  "homepage": "https://github.com/betgar/typescript-babel-eslint#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/lodash-es": "^4.17.3",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-loader": "^8.0.6",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^3.0.2",
    "i": "^0.3.6",
    "lint-staged": "^9.2.1",
    "npm": "^6.10.2",
    "standard-version": "^7.0.0",
    "typescript": "^3.5.3",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "core-js": "^3.1.4",
    "lodash-es": "^4.17.15",
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,}": [
      "npm run lint",
      "git add"
    ]
  },
  "types": "./dist/index.d"
}
